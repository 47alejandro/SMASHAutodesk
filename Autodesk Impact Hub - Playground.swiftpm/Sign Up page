import SwiftUI
struct Tittle: LabelStyle {
    func makeBody(configuration: Configuration) -> some View {
        HStack {
            configuration.icon
                .frame(width: 50)
                .background(.quaternary, in: Circle())
            
            configuration.title
            
            Spacer()
        }
    }
}

